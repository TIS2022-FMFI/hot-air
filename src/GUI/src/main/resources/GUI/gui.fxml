<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.GUIController">
   <center>
      <TabPane minHeight="800.0" minWidth="800.0" prefHeight="800.0" prefWidth="800.0" tabClosingPolicy="UNAVAILABLE"
               BorderPane.alignment="CENTER">
         <Tab text="BLOWERS">
            <BorderPane fx:id="blowers" prefHeight="200.0" prefWidth="200.0">
               <top>
                  <HBox alignment="CENTER" BorderPane.alignment="CENTER">
                     <padding>
                        <Insets left="10.0" right="10.0"/>
                     </padding>
                     <Button fx:id="loadBtn" maxWidth="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
                             onAction="#loadFile" prefHeight="23.0" prefWidth="100.0" text="LOAD FILE">
                        <font>
                           <Font name="Arial Bold" size="12.0"/>
                        </font>
                        <HBox.margin>
                           <Insets bottom="5.0" top="5.0"/>
                        </HBox.margin>
                     </Button>
                     <TextField fx:id="filePath" maxWidth="1.7976931348623157E308" minWidth="-Infinity"
                                prefHeight="25.0" prefWidth="495.0" HBox.hgrow="ALWAYS"/>
                     <Button fx:id="searchBtn" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                             minWidth="-Infinity" mnemonicParsing="false" onAction="#chooseFile" prefHeight="27.0"
                             prefWidth="27.0">
                        <graphic>
                           <ImageView fitHeight="23.0" fitWidth="23.0">
                              <Image url="@search.png"/>
                           </ImageView>
                        </graphic>
                        <HBox.margin>
                           <Insets left="5.0" right="5.0"/>
                        </HBox.margin>
                     </Button>
                     <ImageView fx:id="bogeLogo" fitHeight="45.0" fitWidth="142.0" pickOnBounds="true"
                                preserveRatio="true">
                        <HBox.margin>
                           <Insets left="4.0" right="2.0"/>
                        </HBox.margin>
                        <Image url="@boge.png"/>
                     </ImageView>
                  </HBox>
               </top>
               <center>
                  <GridPane prefHeight="708.0" prefWidth="795.0" BorderPane.alignment="CENTER">
                     <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                     </columnConstraints>
                     <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                     </rowConstraints>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                     </padding>
                     <Rectangle arcHeight="15.0" arcWidth="15.0" fill="#ffffff00" height="229.0" stroke="BLACK"
                                strokeLineCap="ROUND" strokeLineJoin="ROUND" strokeType="INSIDE" width="395.0"/>
                  </GridPane>
               </center>
            </BorderPane>
         </Tab>
         <Tab text="PROJECTS">
            <BorderPane fx:id="projects" prefHeight="200.0" prefWidth="200.0">
               <top>
                  <HBox alignment="CENTER" BorderPane.alignment="CENTER">
                     <padding>
                        <Insets left="10.0" right="10.0"/>
                     </padding>
                     <Button fx:id="loadBtn2" maxWidth="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
                             onAction="#loadFile" prefHeight="23.0" prefWidth="100.0" text="LOAD FILE">
                        <font>
                           <Font name="Arial Bold" size="12.0"/>
                        </font>
                        <HBox.margin>
                           <Insets bottom="5.0" top="5.0"/>
                        </HBox.margin>
                     </Button>
                     <TextField fx:id="filePath2" maxWidth="1.7976931348623157E308" minWidth="-Infinity"
                                prefHeight="25.0" prefWidth="495.0" HBox.hgrow="ALWAYS"/>
                     <Button fx:id="searchBtn2" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                             minWidth="-Infinity" mnemonicParsing="false" onAction="#chooseFile" prefHeight="27.0"
                             prefWidth="27.0">
                        <graphic>
                           <ImageView fitHeight="23.0" fitWidth="23.0">
                              <Image url="@search.png"/>
                           </ImageView>
                        </graphic>
                        <HBox.margin>
                           <Insets left="5.0" right="5.0"/>
                        </HBox.margin>
                     </Button>
                     <ImageView fx:id="bogeLogo2" fitHeight="45.0" fitWidth="142.0" pickOnBounds="true"
                                preserveRatio="true">
                        <HBox.margin>
                           <Insets left="4.0" right="2.0"/>
                        </HBox.margin>
                        <Image url="@boge.png"/>
                     </ImageView>
                  </HBox>
               </top>
            </BorderPane>
         </Tab>
      </TabPane>
   </center>
</BorderPane>
